<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sg.com.bigspoon.www"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/bigspoonmobileappiconwithname"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="sg.com.bigspoon.www.TutorialActivity"
            android:label="@string/title_activity_tutorial"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.TutorialScreen1"
            android:label="@string/title_activity_tutorial_screen1"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.LaunchActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Black.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.EntryActivity"
            android:label="@string/title_activity_entry"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.TutorialScreen6"
            android:label="@string/title_activity_tutorial_screen6"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="sg.com.bigspoon.www.MainActivity"
             android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
    </application>

</manifest>
